.gitignore
include_rules

: foreach *.coffee |> coffee -c %f |> %B.js
: foreach *.scss |> sassc < %f > %o |> %B.css
: foreach *.md |> markdown < %f > %o |> %B.html

: $(TOP)/node_modules/requirejs/require.js $(TOP)/node_modules/preact/dist/preact.js $(TOP)/node_modules/preact-router/dist/preact-router.js $(TOP)/node_modules/classnames/index.js js/define_libs.js js/*.js |> !join |> bundle.js

: foreach *.js |> $(TOP)/node_modules/uglify-js/bin/uglifyjs %f > %o |> %B.min.js



